<?xml version="1.0" encoding="UTF-8"?>
<plugin>
	<name>JSONPlugin</name>
	<class>net.scientifichooliganism.jsonplugin.JSONPlugin</class>
	<configuration>
		<module>JSONPlugin</module>
		<sequence>0</sequence>
		<key>active</key>
		<value>true</value>
	</configuration>
	<configuration>
		<module>JSONPlugin</module>
		<sequence>1</sequence>
		<key>storage</key>
		<value>false</value>
	</configuration>
	<action>
		<name>objectFromJson</name>
		<description>Creates an object from the specified json.</description>
		<module>JSONPlugin</module>
		<class>net.scientifichooliganism.jsonplugin.JSONPlugin</class>
		<!--<url></url>-->
		<method>objectFromJson</method>
	</action>
	<action>
		<name>jsonFromObject</name>
		<description>Creates json from an object.</description>
		<module>JSONPlugin</module>
		<class>net.scientifichooliganism.jsonplugin.JSONPlugin</class>
		<!--<url></url>-->
		<method>jsonFromObject</method>
	</action>
</plugin>